{
  "name": "project-blockify",
  "version": "1.0.0",
  "description": "LEGO E-commerce Platform for Academic Purpose",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "backend:dev": "cd backend && npm run dev",
    "backend:build": "cd backend && npm run build",
    "backend:start": "cd backend && npm start",
    "backend:test": "cd backend && npm test",
    "frontend:dev": "cd Frontend && npm run dev",
    "frontend:preview": "cd Frontend && npm run preview",
    "frontend:build": "cd Frontend && npm run build",
    "setup": "npm install && cd backend && npm install && cd ../Frontend && npm install",
    "build": "npm run backend:build && npm run frontend:build",
    "start": "npm run backend:start",
    "start:all": "concurrently \"npm run backend:start\" \"npm run frontend:preview\""
  },
  "keywords": [
    "lego",
    "ecommerce",
    "academic",
    "nodejs",
    "typescript",
    "bootstrap",
    "ddd",
    "mvc"
  ],
  "author": "PeanLutHuynh",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}